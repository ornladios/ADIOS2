sub output_header {
    $mach = 'arm64';
    open(COUT, ">dill_${mach}.c") || die "Can't open header output";
print COUT<<EOF;
/* This file is generated from arm64.ops.  Do not edit directly. */

#include "dill.h"
#include "dill_internal.h"
#include "arm64.h"
EOF
}
